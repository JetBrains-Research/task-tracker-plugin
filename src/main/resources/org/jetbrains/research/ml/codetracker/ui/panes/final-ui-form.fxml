<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.shape.Rectangle?>

<?import javafx.scene.text.Text?>
<AnchorPane fx:id="mainPane" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane id="codetrackerPane" prefHeight="${80.0 * scale}" prefWidth="${560.0 * scale}" stylesheets="@style.css">
        <Label layoutX="193.0" layoutY="11.0" styleClass="codetracker-sign" text="CodeTracker">
        </Label>
    </AnchorPane>
    <Pane fx:id="finishPane" layoutY="80.0" prefHeight="${760.0 * scale}" prefWidth="${560.0 * scale}">
        <Rectangle fx:id="blueRectangle" arcHeight="5.0" arcWidth="5.0" fill="#7abaf5" height="${232.0 * scale}" layoutY="210.0" opacity="0.25" stroke="WHITE" strokeType="INSIDE" width="${560.0 * scale}" />
        <Polygon fx:id="orangePolygon" fill="#f5d590" layoutX="${60.0 * scale}" layoutY="${314.0 * scale}" opacity="0.41" points="-60.0, 12.0, 500.0, -314.0, -60.0, -314.0" stroke="WHITE" strokeType="INSIDE" />
        <Polygon fx:id="yellowPolygon" fill="#fff782" layoutX="${87.0 * scale}" layoutY="${482.0 * scale}" opacity="0.3" points="-80.0, 30.0, 473.0, 135.0, 473.0, -132.0" stroke="WHITE" strokeType="INSIDE" />
        <Button fx:id="backToTasksButton" layoutX="374.0" layoutY="340.0" mnemonicParsing="false" prefHeight="${93.0 * scale}" prefWidth="${126.0 * scale}">
            <graphic>
                <VBox prefHeight="${106.0 * scale}" prefWidth="${122.0 * scale}" styleClass="v-box">
                    <ImageView fitheight="${35.0 * scale}" fitwidth="${75.0 * scale}" pickOnBounds="true" preserveRatio="true">
                        <Image url="@next.png" />
                    </ImageView>
                    <Text fx:id="backToTasksText" strokeType="OUTSIDE" strokeWidth="0.0" text="вернуться к задачам" textAlignment="CENTER" wrappingWidth="${121.736328125 * scale}" styleClass="sanserif-bold-15">
                    </Text>
                </VBox>
            </graphic>
        </Button>
        <Label fx:id="greatWorkLabel" alignment="CENTER" layoutX="144.0" layoutY="154.0" prefHeight="${71.0 * scale}" prefWidth="${280.0 * scale}" styleClass="sanserif-bold-24" textAlignment="CENTER">
        </Label>
        <Text fx:id="messageText" layoutX="98.0" layoutY="261.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="${364.0 * scale}" styleClass="sanserif-20">
        </Text>
        <ComboBox fx:id="languageComboBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="${25.0 * scale}" prefWidth="${40.0 * scale}" layoutX="${510.0 * scale}" layoutY="${8.0 * scale}" styleClass="language" visibleRowCount="5" />
        <Button fx:id="backToProfileButton" layoutX="60.0" layoutY="340.0" mnemonicParsing="false" prefHeight="${93.0 * scale}" prefWidth="${126.0 * scale}">
            <graphic>
                <VBox prefHeight="${106.0 * scale}" prefWidth="${122.0 * scale}" styleClass="v-box">
                    <ImageView fitheight="${35.0 * scale}" fitwidth="${75.0 * scale}" nodeOrientation="RIGHT_TO_LEFT" pickOnBounds="true" preserveRatio="true">
                        <Image url="@next.png" />
                    </ImageView>
                    <Text fx:id="backToProfileText" strokeType="OUTSIDE" strokeWidth="0.0" text="вернуться к анкете " textAlignment="CENTER" wrappingWidth="${121.736328125 * scale}" styleClass="sanserif-bold-15">
                    </Text>
                </VBox>
            </graphic>
        </Button>
    </Pane>
</AnchorPane>
